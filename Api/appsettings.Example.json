{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "HashingOptions": {
    "Iterations": 10000
  },
  "ConnectionStrings": {
    "SupabaseCredentials": "... Supabase DB connection string"
  },
  "JwtSettings": {
    "AccessTokenSecret": "... Secret for hashing access token",
    "RefreshTokenSecret": "... Secret for hashing refresh token",
    "AccessTokenExpirationMinutes": 60,
    "RefreshTokenExpirationMinutes": 10080,
    "Issuer": "... url",
    "Audience": "... url"
  },
  "RateLimitSettings": {
    "PermitLimit": 50,
    "QueueLimit": 0,
    "ReplenishmentPeriod": 10,
    "Window": 10,
    "AutoReplenishment": true
  },
  "SupabaseSettings": {
    "Url": "... url",
    "AnonKey": "... credentials from supabase for storage"
  },
  "GOOGLE_SIGNIN_CLIENT_ID": "... google client id the same as at the front for decoding google id_token"
}
